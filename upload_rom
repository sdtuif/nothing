echo "== Uploading =="
set -e
cd ~/roms/$rom_name
cd out/target/product/$device
ls -lh
pwd
chmod 777 *
curl --upload-file $(echo $rom_name*.zip) https://transfer.sh | tee link.txt && echo " "
echo " "
echo "=== Rom Uploaded! ==="
echo " = Please see the link Below = "
echo "----------------------------"
cat link.txt && echo " "
echo "============================"
